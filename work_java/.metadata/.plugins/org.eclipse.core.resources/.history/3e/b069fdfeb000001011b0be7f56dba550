package backjoon;

import java.util.Scanner;

public class q_1000 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int A= scanner.nextInt();
		int B= scanner.nextInt();
		System.out.println(A+B);
		System.out.println(A-B);
		System.out.println(A*B);
		System.out.println((double)A/B);
		System.out.println((double)A%B);
		scanner.close();
	}
}
